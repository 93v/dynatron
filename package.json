{
  "name": "dynatron",
  "version": "0.0.0-development",
  "description": "Tools for working with DynamoDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watchAll --passWithNoTests",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "npm link && nodemon",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/93v/dynatron.git"
  },
  "keywords": [
    "AWS DynamoDB",
    "DocumentClient",
    "AWS Lambda",
    "Serverless",
    "Dynatron"
  ],
  "author": {
    "name": "93v",
    "email": "93v@protonmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/93v/dynatron/issues"
  },
  "homepage": "https://93v.github.io/dynatron/",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint && npm test"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/async-retry": "^1.4.2",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "aws-sdk": "^2.753.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "semantic-release": "^17.1.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "dist/**/*",
    "types/**/*"
  ],
  "dependencies": {
    "async-retry": "^1.3.1",
    "fast-deep-equal": "^3.1.3",
    "uuid": "^8.3.0"
  }
}
