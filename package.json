{
  "name": "dynatron",
  "version": "0.0.0-development",
  "description": "Tools for working with DynamoDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "test": "jest --forceExit",
    "test:watch": "jest --watchAll",
    "build": "rm -rf ./dist && webpack",
    "watch": "rm -rf ./dist && webpack -w",
    "start": "npm link && nodemon",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/93v/dynatron.git"
  },
  "keywords": [
    "AWS DynamoDB",
    "DocumentClient",
    "AWS Lambda",
    "Serverless",
    "Dynatron"
  ],
  "author": {
    "name": "93v",
    "email": "93v@protonmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/93v/dynatron/issues"
  },
  "homepage": "https://93v.github.io/dynatron/",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint && npm test"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/async-retry": "^1.4.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-unicorn": "^26.0.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "nock": "^13.0.6",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.7",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-node-externals": "^2.5.2"
  },
  "files": [
    "dist/**/*",
    "types/**/*"
  ],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.3.0",
    "@aws-sdk/node-http-handler": "^3.3.0",
    "@aws-sdk/shared-ini-file-loader": "^3.1.0",
    "@aws-sdk/util-dynamodb": "^3.3.0",
    "async-retry": "^1.3.1",
    "fast-deep-equal": "^3.1.3"
  }
}
