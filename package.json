{
  "name": "dynatron",
  "version": "0.0.0-development",
  "description": "Tools for working with DynamoDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "test": "jest --forceExit",
    "test:watch": "jest --watchAll",
    "build": "rm -rf ./dist && webpack",
    "watch": "rm -rf ./dist && webpack -w",
    "start": "npm link && nodemon",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/93v/dynatron.git"
  },
  "keywords": [
    "AWS DynamoDB",
    "DocumentClient",
    "AWS Lambda",
    "Serverless",
    "Dynatron"
  ],
  "author": {
    "name": "93v",
    "email": "93v@protonmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/93v/dynatron/issues"
  },
  "homepage": "https://93v.github.io/dynatron/",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint && npm test"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/async-retry": "^1.4.2",
    "@types/ini": "^1.3.30",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-unicorn": "^29.0.0",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "nock": "^13.0.11",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.27.1",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "files": [
    "dist/**/*",
    "types/**/*"
  ],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.9.0",
    "@aws-sdk/node-http-handler": "^3.6.1",
    "@aws-sdk/util-dynamodb": "^3.9.0",
    "async-retry": "^1.3.1",
    "ini": "^2.0.0"
  }
}
